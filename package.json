{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "export": "next export",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@babel/core": "^7.16.12",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.25.0",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "sass": "^1.49.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/styled-components": "^5.1.21",
    "babel-jest": "^27.4.6",
    "babel-plugin-styled-components": "^2.0.2",
    "cypress": "^9.3.1",
    "gts": "^3.1.0",
    "jest": "^27.4.7",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "src"
    ],
    "roots": [
      "<rootDir>"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/",
      "<rootDir>/cypress/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  }
}
